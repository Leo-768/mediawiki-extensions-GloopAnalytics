<div class="gloopanalytics-data">
	<div id="gloopanalytics-page-views">
		<div id="gloopanalytics-page-views-all">
			<h3>All page views (30d)</h3>
			<div class="gloopanalytics-chart"><canvas></canvas></div>
		</div>
		<div id="gloopanalytics-page-views-single" data-initial="{{ initial_page }}">
			<h3>Page views for specific page (30d)</h3>
			<div id="gloopanalytics-page-views-form"></div>
			<div class="gloopanalytics-chart"></div>
		</div>
	</div>
	<div id="gloopanalytics-top-pages">
		<h3>Top pages (30d)</h3>
		<table class="wikitable">
			<thead>
				<tr>
					<th>Path</th>
					<th>Requests</th>
				</tr>
			</thead>
			<tbody></tbody>
		</table>
	</div>
</div>
